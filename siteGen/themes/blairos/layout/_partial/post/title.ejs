<% if (post.link){ %>
  <h1 itemprop="name">
    <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%= post.title %></a>
    <time datetime="<%= date_xml(post.date) %>" itemprop="datePublished"><%= date(post.date, date_format) %></time>
  </h1>
<% } else if (post.title){ %>
  <% if (index){ %>
    <h4 class="myh4"  itemprop="name">
      <span>
         <!-- <%= post.date%> -->
      </span>
      <a class="<%= class_name %>" href="<%- url_for(post.path) %>"><%= post.title %></a>
      <!--<a>
      <div = post.date  </div>
      </a>-->
      <!-- date(post.date) -->
      <!--</li>-->
    </h4>
  <% } else { %>
    <h1 class="<%= class_name %>" itemprop="name">
      <%= post.title %>
    </h1>
  <% } %>
<% } else if (post.papertitle){ %>
  <h1 class="<%= class_name %>" itemprop="name">
    <%= post.papertitle %>
  </h1>
<% } %>
